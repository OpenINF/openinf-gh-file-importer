{
  "name": "@openinf/gh-file-importer",
  "version": "2.0.1",
  "description": "Utility that imports arbitrary files from remote GitHub repositories",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "prepare": "npm run compile-src && npm run compile-doc",
    "compile-src": "tsc --project ./",
    "compile-doc": "jsdoc2md --template README.hbs --files index.js > ./README.md",
    "test": "node index.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openinf/gh-file-importer.git"
  },
  "keywords": [
    "github",
    "repository",
    "file",
    "download"
  ],
  "author": "The OpenINF Authors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openinf/gh-file-importer/issues"
  },
  "homepage": "https://github.com/openinf/gh-file-importer#readme",
  "dependencies": {
    "@octokit/rest": "19.0.5",
    "@openinf/util-errors": "2.0.7",
    "@openinf/util-text": "1.1.2",
    "console-log-level": "1.4.1",
    "node-fetch": "3.2.10"
  },
  "devDependencies": {
    "@openinf/util-object": "2.0.1",
    "@types/console-log-level": "1.4.2",
    "@types/node": "18.8.5",
    "@types/node-fetch": "3.0.2",
    "jsdoc-to-markdown": "7.1.1"
  }
}
